{
  "pages": [
    "pages/index/index",
    "pages/login/login",
    "pages/history/history",
    "pages/wish/wish",
    "pages/history/vlog/vlog",
    "pages/my/my"
  ],
  "subpackages": [
    {
      "root": "pages/weatherPkg/",
      "pages": [
        "components/ec-canvas",
        "weather/weather"
      ]
    }
  ],
  "window": {
    "backgroundTextStyle": "light",
